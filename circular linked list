#include<stdio.h>
#include<stdli.h>
#define SIZE 5
int cqueue[size], f = 0, r = 0, count = 0;
void enq();
void deq();
void display();
int main()
{
	int ch;
	while(1)
	{
		printf("\n1.Enq 2.Enq 3.Display 4.Exit\n");
		printf("Enter your choice:");
		scanf("%d",&ch);
		swich(ch)
		{
			case 1: enq();break;
			case 2: enq();break;
			case 3: display();break;
			case 4: exit(0);
			default: printf("Invalid choice\n");
		}
	}
	return 0;
}
void enq()
{
	if (count == size){
		printf("Queue is Full\n");
	}
	else
	{
		int ele;
		printf("Enter an element:");
		scanf("%d",&ele);
		cqueue[r] = ele;
		r++;
		if(r>4)
		   r=0;
		count++;
	}
}
void dnq()
{
	if (count == 0){
		printf("Queue is empty\n");
	}
	else
	{
		int ele = cqueue[f];
		r++;
		if(r>4)
		   r=0;
		count--;
		printf("Deleted element is %d\n",ele)
	}
}
void display()
{
	if (count == 0){
		printf("Queue is empty\n");
	}
	else
	{
		printf("Queue is empty\n");
		int i, temp = f;
		for(i=0; i < count; i++)
		{
			printf("%d",cqueue[temp]);
			temp++;
			if(temp>4)temp = 0;
		
		}
		printf("\n");
	}
}
	

